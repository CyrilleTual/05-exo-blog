<div class="half">
    <H1> Voici tout nos articles : </H1>
    
    <% data.forEach(element=> { %>
    
        <article class="card">
            <h3>
                <%= element.title %>
            </h3>
            <% element.photos.forEach(element=> { %>
                <img src="/img/<%=element.url  %>" alt="<%=element.url %>">
                <% }) %>
            <p>
                Un récit de <%= element.alias %>  posté le <%= element.date.toLocaleDateString() %> dans la catégorie <%= element.category %>
            </p>
            <p>
                <%= element.content %>
            </p>
            <a href="/story/<%= element.storyID%>">En savoir plus</a>
    
        </article>
    
        <% }) %>

</div>

