<div class="half">
    <H1> Voici tout nos articles : </H1>
    
    <% data.forEach(element=> { %>
    
        <article class="card">
            <h3>
                <%= element.title %>
            </h3>
            <p>
                <%= element.date %>
            </p>
            <a href="/story/<%= element.id%>">En savoir plus</a>
    
        </article>
    
        <% }) %>

</div>

