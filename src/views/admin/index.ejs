<div class="half">
    <H1> Voici la liste de tout nos articles : </H1>

    <a href="/admin/createStory/<%=  %>">Ecrire un post</a>

    <% data.forEach(element=> { %>

        <article class="card">
            <h3>
                <%= element.title %>
            </h3>
            <p>
                <%= element.alias %>
            </p>
            <p>
                <%= element.content %>
            </p>
            <a href="/admin/edit/<%= element.storyID%>">edit</a>
            <a href="/admin/delete/story/<%= element.storyID%>">delete</a>
        </article>

        <% }) %>

</div>